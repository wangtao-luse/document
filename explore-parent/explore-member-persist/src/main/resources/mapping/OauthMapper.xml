<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.explore.member.persist.OauthMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.explore.model.member.Oauth">
        <id column="id" property="id" />
        <result column="uid" property="uid" />
        <result column="oauth_id" property="oauthId" />
        <result column="oauth_type" property="oauthType" />
        <result column="credential" property="credential" />
        <result column="nickname" property="nickname" />
        <result column="avatar" property="avatar" />
        <result column="passwd" property="passwd" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, uid, oauth_id, oauth_type, credential, nickname, avatar, passwd
    </sql>

</mapper>
